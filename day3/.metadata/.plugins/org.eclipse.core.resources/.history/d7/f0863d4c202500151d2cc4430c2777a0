package cy.static学习;

public class Demo01 {
	
	int age;
	
    void hello(){
		System.out.println("hello...");
	}
	//类方法
	static void hi()
	{
		System.out.println("hi...");
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Demo01 d=new Demo01();
		d.age=10;
		d.hello();

	}

}
