package cy.shuzhi;

public class Exe03 {

	/**
	 * @param args
	 * 创建Exe03.java：创建每一种数值型变量，赋值，进行各种“加减乘除”的运算
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=5;
		int b=8;
		System.out.println("a+b="+(a+b));
		System.out.println("a*b="+a*b);
		System.out.println("a/b="+a/b);
		

	}

}
