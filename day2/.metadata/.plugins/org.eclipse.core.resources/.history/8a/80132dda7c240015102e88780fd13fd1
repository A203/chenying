package cy.loop.ex;

public class zhishu {

	/**
	 * @param args
	 * 打印2-50的质数
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		boolean pan=true;
		for(int i=2;i<=50;i++)
		{
			for(int j=2;j<=i;j++)
			{
				if(i%j==0)
					pan=false;
					break;
			}
			if(pan)
			System.out.println(i);
		}

	}

}
