package cy.continuebreak;

public class Continuedemo {

	/**
	 * @param args
	 * 打印1-99之间能够被5整除的数并记录不符合要求的数字的个数
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i,count=0;
		for(i=1;i<=99;i++)
		{
			if(i%5==0)
			{
				System.out.println("i="+i);
				continue;
			}
			count++;
		}

	}

}
