package cy.loop.ex;

public class zhishu {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		boolean pan=true;
		for(int i=2;i<=50;i++)
		{
			for(int j=2;j<=i;j++)
			{
				if(j%i==0)
				{
					pan=false;
					break;
				}
			}
			System.out.println(i+"ÊÇÖÊÊý£¿"+pan);
		}

	}

}
